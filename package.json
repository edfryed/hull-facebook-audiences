{
  "name": "hull-facebook-audiences",
  "description": "Target your Leads with Facebook Ads",
  "version": "0.2.3",
  "homepage": "https://github.com/hull-ships/hull-facebook-audiences",
  "license": "MIT",
  "author": {
    "name": "Hull",
    "email": "contact@hull.io",
    "url": "https://github.com/hull"
  },
  "engines": {
    "node": "8.10.x",
    "npm": "5.6.x",
    "yarn": "1.5.x"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hull-ships/hull-facebook-audiences"
  },
  "bugs": {
    "url": "https://github.com/hull-ships/hull-facebook-audiences/issues"
  },
  "scripts": {
    "build": "npm run clean && npm run build:server",
    "build:server": "babel server -d build",
    "clean": "rimraf dist; rimraf build",
    "start": "node -r newrelic ./build",
    "start:dev": "NODE_ENV=development babel-watch -L server",
    "test": "npm run test:lint && npm run test:manifest && npm run test:unit && npm run test:integration",
    "test:lint": "eslint server",
    "test:manifest": "jsonlint manifest.json -q",
    "test:unit": "NODE_ENV=test mocha -r babel-register -R spec ./test/unit/*.js",
    "test:integration": "NODE_ENV=test mocha -r babel-register -R spec ./test/integration/*.js",
    "test:flow": "flow check",
    "watch": "watch 'clear && npm run test:lint -s && npm run test:unit -s' server src",
    "postinstall": "npm run build"
  },
  "keywords": [
    "hull",
    "ship",
    "hull-ship",
    "facebook"
  ],
  "dependencies": {
    "babel-cli": "^6.10.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "bluebird": "^3.3.4",
    "cache-manager-redis-store": "^1.2.0",
    "country-data": "0.0.31",
    "express": "^4.14.0",
    "fbgraph": "^1.2.0",
    "hull": "0.13.10",
    "lodash": "^4.13.1",
    "newrelic": "^2.7.1",
    "rimraf": "^2.4.3",
    "urijs": "^1.17.1"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.0",
    "babel-watch": "^2.0.7",
    "eslint": "^2.13.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-flowtype": "^2.44.0",
    "eslint-plugin-flowtype-errors": "^3.3.7",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jest": "^21.12.1",
    "estraverse-fb": "^1.3.1",
    "flow-bin": "^0.63.1",
    "hull-connector-tester": "0.0.6",
    "jsonlint": "^1.6.3",
    "minihull": "2.0.1",
    "mocha": "^2.5.3",
    "nock": "^9.0.14",
    "sinon": "^1.17.6",
    "webpack": "^1.14.0"
  }
}
